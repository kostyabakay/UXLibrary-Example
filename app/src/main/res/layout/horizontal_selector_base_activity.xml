<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.realwear.ux.view.CommandButton
            android:id="@+id/fragment_example_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:onClick="fragmentExampleClick"
            android:text="@string/fragment_example_button" />

        <com.realwear.ux.view.CommandButton
            android:id="@+id/activity_example_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:onClick="activityExampleClick"
            android:text="@string/activity_example_button" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>